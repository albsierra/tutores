<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IES2Mares\tutores\InformacionAlumnosBundle\Entity\Usuario" table="usuario">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tipousuario" type="string" column="tipoUsuario" length="1" nullable="true"/>
    <field name="apellidos" type="string" column="apellidos" length="50" nullable="false"/>
    <field name="nombre" type="string" column="nombre" length="30" nullable="false"/>
    <field name="username" type="string" column="username" length="255" nullable="false"/>
    <field name="password" type="string" column="password" length="255" nullable="false"/>
    <field name="email" type="string" column="email" length="255" nullable="false"/>
    <field name="enabled" type="boolean" column="enabled" nullable="false"/>
  </entity>
</doctrine-mapping>
